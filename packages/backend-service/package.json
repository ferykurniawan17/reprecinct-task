{
  "name": "@reprecinct/backend-service",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules ./src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:swagger": "./test-swagger.sh",
    "docs": "echo 'API documentation available at http://localhost:4000/api-docs when server is running'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "deploy": "./deploy.sh",
    "deploy:quick": "./quick-deploy.sh",
    "deploy:simple": "./deploy-with-env.sh",
    "deploy:interactive": "./deploy-with-env.sh --interactive",
    "deploy:one-click": "./one-click-deploy.sh",
    "fix:s3": "./fix-s3-bucket.sh",
    "setup:rds": "./setup-rds.sh",
    "test:local": "./test-local.sh",
    "cleanup": "./cleanup.sh",
    "aws:validate": "sam validate",
    "aws:build": "sam build",
    "aws:local": "sam local start-api",
    "aws:logs": "sam logs -n BackendFunction --stack-name reprecinct-backend-simple --tail"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vendia/serverless-express": "^4.12.6",
    "prisma": "6.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
